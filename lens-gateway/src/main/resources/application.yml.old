spring:
  cloud:
    gateway:
      discovery:
        locator:
          enabled: true
        lowerCaseServiceId: true
gateway:
  routes:
    nacos:
      namespace: public
      dataId: sc-routes
      group: DEFAULT_GROUP
      routes:
        - id: lens-gateway
          uri: lb://lens-gateway
          predicates:
            - Path=/v1/gateway/**
          filters:
            - StripPrefix=2
        - id: gzh-server
          uri: lb://gzh-server
          predicates:
            - Path=/v1/wx/gzhserver/**
          filters:
            - StripPrefix=3